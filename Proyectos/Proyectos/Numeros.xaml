<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proyectos.Numeros">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Numeros"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button
                x:Name="Calcular_Numeros"
                Clicked="Calcular_Numeros_Clicked"
                FontFamily="ubutu"
                ></Button>
            <Label
               x:Name="num" 
               VerticalOptions="CenterAndExpand"
                HorizontalOptions="CenterAndExpand"
                >
            </Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>